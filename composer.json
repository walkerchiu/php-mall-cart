{
    "name": "walkerchiu/php-mall-cart",
    "type": "library",
    "authors": [
        {
            "name":  "Walker Chiu",
            "email": "chenjen.chiou@gmail.com"
        }
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3",
        "walkerchiu/php-core": "^1.0",
        "walkerchiu/php-morph-image": "^1.0"
    },
    "suggest": {
        "walkerchiu/php-mall-order": "^1.0",
        "walkerchiu/php-mall-shelf": "^1.0",
        "walkerchiu/php-site": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "WalkerChiu\\MallCart\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WalkerChiu\\MallCart\\MallCartServiceProvider"
            ],
            "aliases": {
                "Channel": "WalkerChiu\\MallCart\\Models\\Entities\\Channel",
                "Item": "WalkerChiu\\MallCart\\Models\\Entities\\Item"
            }
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
